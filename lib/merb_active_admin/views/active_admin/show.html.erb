<table class="active_admin_object_form">
  <%- @object.class.columns.reject{ |c| @object.class.primary_key == c }.each do |col| -%>
  <tr>
    <td class="active_admin_object_key"><%= underscored_to_human_readable(col) %></td>
    <td class="active_admin_object_value"><%= @object.send(col) %></td>
  </tr>
  <%- end -%>
</table>
