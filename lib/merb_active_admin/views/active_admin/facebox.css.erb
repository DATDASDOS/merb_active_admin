#facebox .b {
  background:url(<%= active_admin_file_url("facebox/images/b.png") %>);
}

#facebox .tl {
  background:url(<%= active_admin_file_url("facebox/images/tl.png") %>);
}

#facebox .tr {
  background:url(<%= active_admin_file_url("facebox/images/tr.png") %>);
}

#facebox .bl {
  background:url(<%= active_admin_file_url("facebox/images/bl.png") %>);
}

#facebox .br {
  background:url(<%= active_admin_file_url("facebox/images/br.png") %>);
}

#facebox {
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 100;
  text-align: left;
}

#facebox .popup {
  position: relative;
}

#facebox table {
  margin: auto;
  border-collapse: collapse;
}

#facebox .body {
  padding: 10px;
  background: #fff;
  width: 370px;
}

#facebox .loading {
  text-align: center;
}

#facebox .image {
  text-align: center;
}

#facebox img {
  border: 0;
}

#facebox .footer {
  border-top: 1px solid #DDDDDD;
  padding-top: 5px;
  margin-top: 10px;
  text-align: right;
}

#facebox .tl, #facebox .tr, #facebox .bl, #facebox .br {
  height: 10px;
  width: 10px;
  overflow: hidden;
  padding: 0;
}

#facebox a.close {
  width: 66px;
  height: 22px;
  display: block;
  text-decoration: none;
  background:url(<%= active_admin_file_url("facebox/images/closelabel.gif") %>);
}

#facebox .dialog {
}

#facebox .buttons {
  margin-top: 8px;
  text-align: center;
}

#facebox a {
  text-align: center;
  text-decoration: none;
  padding: 2px 10px;
  font-size: 14px;
  font-weight: bold;
  color: #7aad00;
}

#facebox .buttons a:hover {
  background-color: #e2ff4e;
}